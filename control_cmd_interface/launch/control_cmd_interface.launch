<?xml version="1.0"?>
<launch>

	<!-- default arguments -->
	<arg name="speech_topic"	 default="/nl_command_parsed" />
	<arg name="service_topic"	 default="/cmd_interface_service" />


	<node name="control_cmd_interface" pkg="control_cmd_interface" type="control_cmd_interface_node" required="true" output="screen">
		<param name="speech_topic" 	value="$(arg speech_topic)"/>
  		<param name="service_topic" 	value="$(arg service_topic)" />
	</node>	


</launch>
